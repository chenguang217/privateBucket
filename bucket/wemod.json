{
    "version": "7.0.1",
    "description": "PC Game Cheats, Trainers, and Mods in One App",
    "homepage": "https://www.wemod.com/",
	"url": "https://api.wemod.com/client/channels/stable/releases/latest#dl.7z",
	"hash": "",
	"pre_install": [
        "$source = $dir.ToString() + '\\WeMod-7.0.19-full.nupkg'",
		"$target = $dir.ToString()",
		"echo $target",
		"7z x $source 'lib/net45/*' \"-o$target\"",
		"$source = $dir.ToString() + '\\lib\\net45\\*'",
		"mv $source $target",
		"$source = $dir.ToString() + '\\RELEASES'",
		"Remove-Item $source",
		"$source = $dir.ToString() + '\\Update.exe'",
		"Remove-Item $source",
		"$source = $dir.ToString() + '\\WeMod-7.0.19-full.nupkg'",
		"Remove-Item $source",
		"$source = $dir.ToString() + '\\background.gif'",
		"Remove-Item $source",
		"$source = $dir.ToString() + '\\setupIcon.ico'",
		"Remove-Item $source"
    ],
	"shortcuts":[
		[
			"WeMod.exe",
			"WeMod"
		]
	]
}