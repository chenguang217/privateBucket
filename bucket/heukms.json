{
	"version": "27.0.1",
	"description": "a KMS activator",
	"homepage": "https://github.com/zbezj/HEU_KMS_Activator",
	"url": "https://hub.gitfast.tk/zbezj/HEU_KMS_Activator/releases/download/27.0.1/HEU_KMS_Activator_v27.0.1.zip#/dl.zip_",
	"hash": "md5:a28d4ae9a5e906e4009652473660a528",
	"pre_install": [
		"Start-Process powershell \"Set-MpPreference -DisableRealtimeMonitoring 1;Add-MpPreference -ExclusionPath $dir\\HEU_KMS_Activator_v27.0.1.exe;Set-MpPreference -DisableRealtimeMonitoring 0\" -Verb RunAs -WindowStyle hidden -wait",
		"7z x $dir\\dl.zip_ -pHEU168 $('-o' + $dir)",
		"'Start-Process powershell \"Set-MpPreference -DisableRealtimeMonitoring 1;Start-Process ' + $dir + '\\HEU_KMS_Activator_v27.0.1.exe;Start-Sleep 30;Set-MpPreference -DisableRealtimeMonitoring 0;\" -Verb RunAs -WindowStyle Hidden' | out-file $dir/test.ps1 -encoding ascii",
		"'@set Path=%Path%;%SystemRoot%\\system32\\WindowsPowerShell\\v1.0\\ & powershell -ExecutionPolicy Unrestricted -NoProfile %~dpn0.ps1 -Verb RunAs -WindowStyle Hidden' | out-file $dir/test.cmd -encoding ascii",
		"remove-item $dir\\dl.zip_"
	],
	"shortcuts": [
		[
			"test.cmd",
			"HEU_KMS_Activator",
			"",
			"HEU_KMS_Activator_v27.0.1.exe"
		]
	],
	"uninstaller": {
		"script": [
			"Start-Process powershell \"Remove-MpPreference -ExclusionPath $dir\\HEU_KMS_Activator_v27.0.1.exe\" -Verb RunAs -WindowStyle hidden -wait"
		]
	}
}